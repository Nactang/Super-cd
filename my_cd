c() {	
	if [ "$1" = ".." ] || [ "${1:0:1}" = "/" ]
	then
		cd $1
		return 0
	fi
	match=`ls -a -d */ | grep "$1"`
	count=""
	for f in ${match[@]}
	do	
		
		if [ "$count" = "1" ] 
		then
			
			select folder in ${match[@]}
			do
				cd $folder
				return 0
			done 
		fi	
		count="1"
	done
	if [ "$match" = "" ]
	then
		cd $1
	else
		cd $match
	fi
	return 0
}
